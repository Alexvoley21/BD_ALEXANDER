USE AF_BD;

CREATE TABLE estudiante (
id int primary key,
nombre varchar(50),
fechaNacimiento date
);

select * from estudiante ;

insert into estudiante (id, nombre , fechaNacimiento)values
(1,'Alexander','2006-11-06'),(2,'Damir','2006-11-10');

DELETE FROM estudiante
where id=1;

select * from estudiante ;

CREATE TABLE materias(
codigo int primary key,
nombre varchar (50)
);

select * from materias;

insert into materias(codigo,nombre)values(40,'Educacion Fisica');
